{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "semi": "off",
    "arrow-parens": "off",
    "comma-dangle": "off",
    "no-console": "off",
    "require-jsdoc": "off",
    "no-trailing-spaces": "off",
    "indent": "off",
    "no-debugger": "off",
    "padded-blocks": "off",
    "prefer-default-exports": "off",
    "no-prototype-builtins": "off"
  },
  "settings": {
    "import/resolver": {
      "alias": {
        "map": [["@", "./src"]]
      },
      "extensions": [".js"]
    }
  }
}
