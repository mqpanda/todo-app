{
  "arrowParens": "avoid",
  "singleQuote": true,
  "semi": false,
  "trailingComma": "all",
  "importOrder":[
    "<THIRD_PARTY_MODULES>",
    "^@/core/(.*)$",
    "^@/core/component(.*)$",
    "^@/components/(.*)$",
    "^@/components/layout/(.*)$",
    "^@/components/screens/(.*)$",
    "^@/styles/(.*)$",
    "^@/config/(.*)$",
    "^@/utils/(.*)$",
    "^@/services/(.*)$",
    "^@/store/(.*)$",
    "^@/api/(.*)$",
    "^.+\\.(template\\.html|module\\.scss)$",
    "^../(.*)",
    "^./(.*)"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true

}
